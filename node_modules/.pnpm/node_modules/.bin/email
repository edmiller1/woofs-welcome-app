#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/edwardmiller/Documents/GitHub/woofs-welcome-app/node_modules/.pnpm/react-email@5.2.5/node_modules/react-email/dist/node_modules:/Users/edwardmiller/Documents/GitHub/woofs-welcome-app/node_modules/.pnpm/react-email@5.2.5/node_modules/react-email/node_modules:/Users/edwardmiller/Documents/GitHub/woofs-welcome-app/node_modules/.pnpm/react-email@5.2.5/node_modules:/Users/edwardmiller/Documents/GitHub/woofs-welcome-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/edwardmiller/Documents/GitHub/woofs-welcome-app/node_modules/.pnpm/react-email@5.2.5/node_modules/react-email/dist/node_modules:/Users/edwardmiller/Documents/GitHub/woofs-welcome-app/node_modules/.pnpm/react-email@5.2.5/node_modules/react-email/node_modules:/Users/edwardmiller/Documents/GitHub/woofs-welcome-app/node_modules/.pnpm/react-email@5.2.5/node_modules:/Users/edwardmiller/Documents/GitHub/woofs-welcome-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --experimental-vm-modules --disable-warning=ExperimentalWarning "$basedir/../react-email/dist/index.js" "$@"
else
  exec node  --experimental-vm-modules --disable-warning=ExperimentalWarning "$basedir/../react-email/dist/index.js" "$@"
fi
